{
    "name": "Filter by keyword",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["*://*.youtube.com/*"],
          "js": ["content.js"]
    }
    ]
}